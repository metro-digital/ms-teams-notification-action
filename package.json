{
  "name": "ms-teams-notification-action",
  "version": "0.0.0",
  "private": true,
  "description": "Microsoft Teams notification action",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build --source-map --license licenses.txt src/main.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "integrity": "git diff --quiet ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metro-digital/ms-teams-notification-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "ms-teams-webhook": "^1.0.4"
  },
  "devDependencies": {
    "@fastify/pre-commit": "^2.0.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vercel/ncc": "^0.34.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.1",
    "prettier": "2.7.1",
    "typescript": "^4.8.4"
  },
  "pre-commit": [
    "format-check",
    "lint",
    "build",
    "integrity"
  ]
}
