  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^18.7.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.8.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.2.5",
    "jest-circus": "^26.6.3",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
