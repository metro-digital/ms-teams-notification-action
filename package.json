{
  "name": "ms-teams-notification-action",
  "version": "0.0.0",
  "private": true,
  "description": "Microsoft Teams notification action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
